@inproceedings{ ISI:000233744700056,
Author = {Kuliberda, K and Wislicki, J and Adamus, R and Subieta, K},
Editor = {{Meersman, R and Tari, Z and Herrero, P and Mendez, G and Cavedon, L and Martin, D and Hinze, A and Buchanan, G and Perez, MS and Robles, V and Humble, J and Albani, A and Dietz, JLG and Panetto, H and Scannapieco, M and Halpin, T and Spyns, P and Zaha, JM and Zimanyi, E and Stefanakis, E and Dillon, T and Feng, L and Jarrar, M and Lehmann, J and DeMoor, A and Duval, E and Aroyo, L}},
Title = {{Object-oriented wrapper for relational databases in the data grid
   architecture}},
Booktitle = {{ON THE MOVE TO MEANINGFUL INTERNET SYSTEMS 2005: OTM 2005 WORKSHOPS,
   PROCEEDINGS}},
Series = {{LECTURE NOTES IN COMPUTER SCIENCE}},
Year = {{2005}},
Volume = {{3762}},
Pages = {{367-376}},
Note = {{OTM Confederated International Conference and Workshop, Agia Napa,
   CYPRUS, OCT 31-NOV 04, 2005}},
Organization = {{RMIT Univ; Vrije Univ Brussels, Dept Comp Sci; Interop}},
Abstract = {{The paper presents a solution of the problem of wrapping relational
   databases to an object-oriented business model in the data grid
   architecture. The main problem with this kind of wrappers is how to
   utilize the native SQL query optimizer, which in majority of RDBMS is
   transparent for the users. In our solution we use the stack-based
   approach to query languages, its query language SBQL, updateable
   object-oriented virtual views and the query modification technique. The
   architecture rewrites the front-end 00 query to a semantically
   equivalent back-end query addressing the MO object model that is 1:1
   compatible with the relational model. Then, in the resulting SBQL query
