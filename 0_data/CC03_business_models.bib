@article{ ISI:000400938500006,
Author = {Wang, Jinjiang and Zhang, Laibin and Duan, Lixiang and Gao, Robert X.},
Title = {{A new paradigm of cloud-based predictive maintenance for intelligent
   manufacturing}},
