@article{ ISI:000350904500005,
Author = {Gerasymenko, Violetta and De Clercq, Dirk and Sapienza, Harry J.},
Title = {{CHANGING THE BUSINESS MODEL: EFFECTS OF VENTURE CAPITAL FIRMS AND
   OUTSIDE CEOs ON PORTFOLIO COMPANY PERFORMANCE}},
