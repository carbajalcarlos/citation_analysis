@article{ ISI:000270078800021,
Author = {Martinez-Olvera, Cesar},
Title = {{Benefits of using hybrid business models within a supply chain}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
