@article{ ISI:000255576400004,
Author = {Lannoo, Bart and Van Ooteghem, Jan and Pareit, Daan and Van Leeuwen, Tom
   and Colle, Didier and Moerman, Ingrid and Demeester, Piet},
Title = {{Business model for broadband Internet on the train}},
Journal = {{JOURNAL OF THE INSTITUTE OF TELECOMMUNICATIONS PROFESSIONALS}},
Year = {{2007}},
Volume = {{1}},
Pages = {{19-27}},
Month = {{JUL-SEP}},
Note = {{46th Annual Congress of the
   Federation-of-Telecommunications-Engineers-of-the-European-Union
   (FITCE), Warsaw, POLAND, AUG 30-SEP 01, 2007}},
Organization = {{Federat Telecommun Engineers European Union}},
Abstract = {{The main goal of this study is to investigate the possibilities and the
   economic viability of offering broadband Internet access on trains. Most
   railway operators and several telecommunications network integrators are
   currently executing feasibility studies and field trials in this area.
   Some commercial services have already been successfully deployed.
   However, these first roll-outs are deployed on a limited route or only
   offer limited bandwidths and quality of service to the passengers. We
   will examine and compare the outdoor technologies and scenarios
   currently available on the market that offer broadband `Internet on the
   train'. Besides the technical solutions, this study also looks at the
   market potential for a nationwide rol-out. Starting from the required
   baindwidth, the number of users and the service adoption, a technology
   can be mapped on to the different parts of the train lines. A complete
   analytical business model has been developed, which is then applied to
   the Belgian railway situation to assess the profitability and
   sensitivity of a specific business case.}},
