@article{ ISI:000371682100005,
Author = {Tantalo, Caterina and Priem, Richard L.},
Title = {{VALUE CREATION THROUGH STAKEHOLDER SYNERGY}},
Journal = {{STRATEGIC MANAGEMENT JOURNAL}},
