@article{ ISI:000400854600005,
Author = {Inigo, Edurne A. and Albareda, Laura and Ritala, Paavo},
Title = {{Business model innovation for sustainability: exploring evolutionary and
   radical approaches through dynamic capabilities}},
Journal = {{INDUSTRY AND INNOVATION}},
Year = {{2017}},
Volume = {{24}},
Number = {{5, SI}},
Pages = {{515-542}},
Note = {{DRUID Summer Conference, LUISS, Rome, ITALY, 2015}},
Abstract = {{The purpose of this paper is to examine the organisational processes of
   business model innovation for sustainability (BMIS). Building on dynamic
   capabilities literature, BMIS is conceptualised to evolutionary and
   radical approaches, and disaggregated into different sensing, seizing
   and reconfiguring capabilities. Based on this conceptual framework, we
   examine a qualitative multiple case study of eight companies in the
   Spanish Basque region, and provide a detailed analysis of how
   organisational and managerial capabilities for sustainability are
   utilised in business model innovation and transformation. These insights
   provide novel theoretical and practical insights into the business
   model, innovation and sustainability literatures.}},
