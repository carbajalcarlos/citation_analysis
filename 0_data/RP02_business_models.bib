@article{ ISI:000375163300008,
Author = {Dijk, Marc and Wells, Peter and Kemp, Rene},
Title = {{Will the momentum of the electric car last? Testing an hypothesis on
   disruptive innovation}},
Journal = {{TECHNOLOGICAL FORECASTING AND SOCIAL CHANGE}},
Year = {{2016}},
Volume = {{105}},
Pages = {{77-88}},
Month = {{APR}},
Abstract = {{In this paper we study to what extent electric propulsion is disrupting
   `the order in the automotive industry with six extensions to
   Christensen's notion of disruptive innovation (1997). For decades the
   automotive sector has relied on the internal combustion engine (ICE) as
   the established propulsion technology, but due to environmental
   regulation and geo-political scarcity problems associated with fossil
   fuel use, electric propulsion is increasingly applied as sole or
   additional power source.
   We elaborate the Christensen typology, rooted in industrial analysis,
   with a regime evolution framework based on changes in technology and the
   institutional context of production and use, with special attention to
   consumer perspectives and government regulation. We offer a hypothesis
   for structural conditions for market disruption and test this hypothesis
   against the development trajectory of full -electric vehicles (FEV).
   Drawing on evidence from a range of recent FEV studies, our analysis
   suggests that the disruptive niche of full-electric mobility is
   currently insufficient to displace the ICE regime. (C) 2016 Elsevier
   Inc. All rights reserved.}},
