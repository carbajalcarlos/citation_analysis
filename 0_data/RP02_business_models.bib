Language = {{English}},
Affiliation = {{Agrawal, N (Reprint Author), ABV IIITM, Cloud \& Cyber Secur Lab, Block F,Morena Link Rd, Gwalior 474015, MP, India.
   Agrawal, Neha; Tapaswi, Shashikala, Atal Bihari Vajpayee Indian Inst Informat Technol, Gwalior, Madhya Pradesh, India.}},
DOI = {{10.1080/19393555.2017.1282995}},
ISSN = {{1939-3555}},
EISSN = {{1939-3547}},
