@incollection{ ISI:000411239000010,
Author = {Straehle, Jochen and Matthaei, Franziska Sophie},
Editor = {{Strahle, J}},
Title = {{The Value Chain of a Branded Second Hand Store-Possible Activities to Be
   Integrated by a Conventional Fashion Brand}},
Booktitle = {{GREEN FASHION RETAIL}},
Series = {{Springer Series in Fashion Business}},
Year = {{2017}},
Pages = {{175-198}},
Abstract = {{The purpose of this paper is to study the recycling form of reusing
   second hand clothing from a conventional fashion brand's perspective. It
   should clarify which measures and activities a fashion company needs to
   integrate in its value chain in order to offer branded second hand
   merchandise in a self-operated store. The research paper relies on a
   desk-based research and aims to illustrate the topic by means of a
   descriptive approach, processing the existing literature. Key findings
   demonstrate that fashion brands need to integrate complete lifecycle
   strategies, sustainability communication, and reverse logistics
   structures, like take-back schemes, for offering second hand clothing.
   The main limitations evolve from the research design. Further, empirical
   evidences need to be conducted for a more fundamental understanding of
   the new business model.}},
