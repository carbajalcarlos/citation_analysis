@article{ ISI:000407792900003,
Author = {Salonen, Anna and Saglam, Onur and Hacklin, Fredrik},
Title = {{Servitization as reinforcement, not transformation}},
Journal = {{JOURNAL OF SERVICE MANAGEMENT}},
