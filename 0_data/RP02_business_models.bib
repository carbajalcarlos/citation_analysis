@article{ ISI:000376675800005,
Author = {Kumar, Sunil and Mittal, Sumit and Singh, Manpreet},
Title = {{FUZZY BASED TRUST MANAGEMENT SYSTEM FOR CLOUD ENVIRONMENT}},
Journal = {{ADVANCES IN SCIENCE AND TECHNOLOGY-RESEARCH JOURNAL}},
Year = {{2016}},
Volume = {{10}},
Number = {{30}},
Pages = {{32-37}},
Month = {{JUN}},
Abstract = {{Cloud computing is a business model with high degree of flexibility,
   scalability in providing infrastructure, platform and software as a
   service over the internet. Cloud promises for easiness and reduced
   expense to service providers and consumers. However, a lack of trust
   between these two stakeholders has hindered the universal acceptance of
   cloud for outsourced services. In this paper, a fuzzy based trust
   management system is proposed to facilitate cloud consumers in
   identifying trustworthy providers. The performance of the proposed
   system is validated through a simulation using CloudAnalyst and
   Simulink.}},
Publisher = {{LUBLIN UNIV TECHNOLOGY, POLAND}},
Address = {{FAC MECHANICAL ENG, NADBYSTRZYCKA 38 D, LUBLIN, 20618, POLAND}},
