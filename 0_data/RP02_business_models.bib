@article{ ISI:000374329200006,
Author = {Le Guyader, Louis P.},
Title = {{A Qualitative Assessment of GE's Performance Prospects}},
Journal = {{JOURNAL OF CORPORATE ACCOUNTING AND FINANCE}},
Year = {{2016}},
Volume = {{27}},
Number = {{3}},
Pages = {{57-62}},
Month = {{MAR-APR}},
Abstract = {{In this article the author discusses the final elements of GE's
   reorganization plan to produce a stronger, streamlined industrial giant
   that has shed an increasingly burdensome finance arm. The simplest
   assessment is that the full benefits of the reorganization may take time
   to emerge as the after-effects of divestitures linger and acquisitions
   take time to close and digest. New risks come with the reorganization;
   and the reorganization and the new business model is exposed to several
   layers of new regulations. The final result may not deliver as clear an
   industrial earnings signal as first expected. The plan may not be
   executed as cleanly as expected, making that signal noisy. (C) 2016
   Wiley Periodicals, Inc.}},
