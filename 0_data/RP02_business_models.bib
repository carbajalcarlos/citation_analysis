@article{ ISI:000387191500042,
Author = {Groslambert, Bertrand and Chiappini, Raphael and Bruno, Olivier},
Title = {{Desperately seeking cash: Evidence from bank output measurement}},
Journal = {{ECONOMIC MODELLING}},
Year = {{2016}},
Volume = {{59}},
Pages = {{495-507}},
Month = {{DEC}},
Abstract = {{We examine the impact of a ``near-zero{''} interest rate policy on bank
   output. Specifically, we document the existence of negative banking
   output on deposits for French banks from 2009. We show a structural
   break in banks' long run interest rate pass-through that explains this
   change in their business model during the 2003-2012 period. Since the
   crisis, banks are desperately seeking cash, and deposits have become a
   cost center. This is due to the new monetary policy and reveals banks'
   adaptation to the new banking regulation on liquidity. This new
   environment raises questions about banks' increasing exposure to
   interest rate risk and shows the necessity of coordinating monetary and
   regulatory policies. (C) 2016 Elsevier B.V. All rights reserved.}},
