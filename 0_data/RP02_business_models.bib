@article{ ISI:000406133400018,
Author = {Wu, Jun and Cui, Xu and Nishi, Tatsushi and Li, Jian},
Title = {{GAME THEORETIC APPROACH FOR COORDINATION OF TELECOMMUNICATION SUPPLY
   CHAIN UNDER NETWORK EXTERNALITY}},
Journal = {{INTERNATIONAL JOURNAL OF INNOVATIVE COMPUTING INFORMATION AND CONTROL}},
Year = {{2016}},
Volume = {{12}},
Number = {{3}},
Pages = {{915-927}},
Month = {{JUN}},
Abstract = {{In view of the booming of telecom industry, we study the coordination
   issues in a telecom service supply chain with network externality.
   First, a mathematical model of the business model in a telecom supply
   chain is established for one mobile provider and one service provider.
   Second, pricing and profit sharing mechanism between telecom operators
   and service providers in the perspective of non-cooperative game and
   cooperative game are analysed respectively. Finally, numerical examples
   are given to support our model. Sonic good insights are presented for
   both agents in the telecom service chain. The effects of system
   parameters on the pricing strategy and the profit allocation mechanism
   are also given.}},
