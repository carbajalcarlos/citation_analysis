@article{ ISI:000387869700001,
Author = {Iwashita, Motoi and Tanimoto, Shigeaki},
Title = {{Service and Billing Management Method for ICT Services}},
Journal = {{INTERNATIONAL JOURNAL OF SOFTWARE INNOVATION}},
Year = {{2016}},
Volume = {{4}},
Number = {{2}},
Pages = {{1-16}},
Month = {{APR-JUN}},
Abstract = {{Information and communication technology (ICT) including broadband
   networks, cloud computing, a variety of mobile devices, point-card
   systems, and ``virtual coins{''} can induce complicated situations
   concerning customer management. In particular, billing management is
   very critical, and it is thus necessary to establish a
   billing-management method that can quickly adjust to changes in the
   environment. The first requirement is managing the relationships between
   services, payments, and settlements so as to assure the payment method
   and service provision and payment routes. Therefore, a business model
   design method, established from the viewpoint of service, cash flow, and
   settlement to strategically adjust to changes in the environment, is
   described herein. In addition, a procedure for managing the connection
   paths is proposed to efficiently construct the relationship between
   services, payments, and settlements.}},
Publisher = {{IGI GLOBAL}},
Address = {{701 E CHOCOLATE AVE, STE 200, HERSHEY, PA 17033-1240 USA}},
