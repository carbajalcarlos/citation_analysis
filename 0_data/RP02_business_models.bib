@article{ ISI:000419821600007,
Author = {Markova, Viera and Lesnikova, Petra and Kascakova, Alena and Vinczeova,
   Miroslava},
Title = {{THE PRESENT STATUS OF SUSTAINABILITY CONCEPT IMPLEMENTATION BY
   BUSINESSES IN SELECTED INDUSTRIES IN THE SLOVAK REPUBLIC}},
Journal = {{E \& M EKONOMIE A MANAGEMENT}},
