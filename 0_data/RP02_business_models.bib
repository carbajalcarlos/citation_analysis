@article{ ISI:000394578800005,
Author = {Shyu, Yih-Wen and Reinfeld, William},
Title = {{Inotera Memories: Leader or Dreamer}},
Journal = {{ASIAN CASE RESEARCH JOURNAL}},
Year = {{2016}},
Volume = {{20}},
Number = {{1}},
Pages = {{111+}},
Month = {{JUN}},
Abstract = {{This case can be divided into two sections. The first section focuses on
   how Inotera Memories can effectively analyze capital expenditure and
   return on investment for long-term capital budgeting in a
   capital-intensive, highly volatile and competitive industry. The
   following section is to highlight the strategy shift of Inotera. The
   DRAM business environment is highly volatile, and continuously requires
   large amounts of capital to improve process technologies, reduce
   production costs and achieve economies of scale. The challenge is
