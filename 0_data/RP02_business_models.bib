@article{ ISI:000400854600002,
Author = {Mongelli, Luca and Rullani, Francesco},
Title = {{Inequality and marginalisation: social innovation, social
   entrepreneurship and business model innovation: The common thread of the
   DRUID Summer Conference 2015}},
Journal = {{INDUSTRY AND INNOVATION}},
Year = {{2017}},
Volume = {{24}},
Number = {{5, SI}},
Pages = {{446-467}},
Note = {{DRUID Summer Conference, LUISS, Rome, ITALY, 2015}},
Abstract = {{Nowadays societies face many societal challenges, among which increasing
   inequality and marginalisation. This paper uses this key to read the
   discussion undertaken during the DRUID Conference 2015 Relevance of
   Innovation', held at LUISS Business School (Rome, IT). We focus on the
   speeches, sessions, and debates that were hosted at the conference. We
   expand them and build on them to advance scholarly discussion on how
   social innovation, social entrepreneurship and business model innovation
   can be used to face inequality and marginalisation. The main idea we
   place at the centre of the discussion is empowerment of marginalised
   individuals by social innovation initiatives and social entrepreneurial
   ventures. Business model innovation is seen as instrumental to this,
   being a crucial tool to foster hybrid organisations and institutions
   able to merge the social and economic dimensions.}},
