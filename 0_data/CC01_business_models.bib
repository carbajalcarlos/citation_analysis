@article{ ISI:000207902800001,
Author = {Birkin, Frank and Polesie, Thomas and Lewis, Linda},
Title = {{A New Business Model for Sustainable Development: an Exploratory Study
   Using the Theory of Constraints in Nordic Organizations}},
Journal = {{BUSINESS STRATEGY AND THE ENVIRONMENT}},
