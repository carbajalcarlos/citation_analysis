@article{ ISI:000290173900016,
Author = {Cheng, Fan-Tien and Yung-Cheng, Jonathan and Huang, Hsien-Cheng and Kao,
   Chi-An and Chen, Ying-Lin and Peng, Ju-Lei},
Title = {{Benefit Model of Virtual Metrology and Integrating AVM into MES}},
Journal = {{IEEE TRANSACTIONS ON SEMICONDUCTOR MANUFACTURING}},
Year = {{2011}},
Volume = {{24}},
Number = {{2}},
Pages = {{261-272}},
Month = {{MAY}},
Note = {{2010 IEEE/SEMI Conference on Advanced Semiconductor Manufacturing, San
   Francisco, CA, JUL 11-13, 2010}},
Organization = {{IEEE; SEMI; Applied Materials; Jordan Valley; Synopsys; Linde; Tefen;
   CPMT; SMA; Tegal; Marcy Nano Ctr}},
Abstract = {{Frequent monitoring in both tool and process is required to detect the
   quality issue early so as to improve the process stability of
   semiconductor manufacturing. However, more tool and process monitoring
   means more metrology operation cost and longer cycle time. Recently, a
   promising technology denoted virtual metrology (VM) has bloomed. VM can
   convert sampling inspection with metrology delay into real-time and
   online total inspection. Therefore, VM has now been designated by
   International SEMATECH Manufacturing Initiative and International
   Technology Road Map for Semiconductors as one of the focus areas for the
   next generation factory realization road map of the semiconductor
   industry. The authors have developed the so-called automatic virtual
   metrology (AVM) system to implement and deploy the VM operations
   automatically. The purpose of this paper is to develop a business model
   to measure the profitability of VM based on in-depth manufacturing
   practices and metrology operations required for semiconductor
   manufacturing. This paper also proposes a novel manufacturing system
   that integrates AVM into the manufacturing execution system (MES). The
   interfaces among AVM, other MES components, and run-to-run (R2R) modules
   in the novel manufacturing system are also defined such that the total
   quality inspection system can be achieved and the R2R capability can be
   migrated from lot-to-lot control to wafer-to-wafer control.}},
