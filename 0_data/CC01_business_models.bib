@article{ ISI:000223050800008,
Author = {Xirogiannis, G and Glykas, M},
Title = {{Fuzzy cognitive maps in business analysis and performance-driven change}},
Journal = {{IEEE TRANSACTIONS ON ENGINEERING MANAGEMENT}},
