@article{ ISI:000330726000006,
Author = {Hennart, Jean-Francois},
Title = {{The Accidental Internationalists: A Theory of Born Globals}},
Journal = {{ENTREPRENEURSHIP THEORY AND PRACTICE}},
Year = {{2014}},
Volume = {{38}},
Number = {{1, SI}},
Pages = {{117-135}},
Month = {{JAN}},
Abstract = {{The distinguishing characteristic of international new ventures/born
   globals (INVs/BGs) is that they have foreign sales from the outset, or
   very quickly afterward. I argue that this is due to their business
   model. INVs/BGs sell to spatially dispersed customers distinctive niche
   products that incur low communication, transportation, and adaptation
   costs. In contrast to the firms described by the Uppsala model, selling
   to foreign customers does not require additional time or effort for
   INVs/BGs. Thus INVs/BGs can be seen as accidental internationalists.}},
