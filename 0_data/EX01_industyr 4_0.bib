@article{ ISI:000351929500003,
Author = {Schueller, Andreas and Epple, Ulrich},
Title = {{A Reference Model for Procedure Description Providing a Basis for
   Industry 4.0}},
Journal = {{AT-AUTOMATISIERUNGSTECHNIK}},
Year = {{2015}},
Volume = {{63}},
Number = {{2, SI}},
Pages = {{87-98}},
Month = {{FEB}},
Abstract = {{A procedure describes a control sequence that is planned for the
   realization of a specific process. From an operational perspective, a
   procedure describes the intended control sequence for a given system
   environment in multiple steps, which include the respective transition
   conditions. The description of procedures is nowadays typically realized
   by using languages that are specially tailored for the given
   application. In this article, we propose the development of a common
   meta-model for procedure descriptions. A meta-model of this kind would
   make a significant contribution to the standardization of procedure
   descriptions. The meta-model has to take care about certain generic
   concepts. These concepts are introduced in this work.}},
