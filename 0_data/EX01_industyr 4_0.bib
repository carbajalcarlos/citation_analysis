@article{ ISI:000426882600016,
Author = {Gehlhoff, Felix and Linnenberg, Tobias and Fay, Alexander},
Title = {{Optimization of auction mechanisms Agent-based coupling of cooperative
   production plants}},
Journal = {{ATP EDITION}},
Year = {{2017}},
Number = {{9}},
Pages = {{54-66}},
Abstract = {{Agent-based control is a promising approach to meet increasing demands
   for flexibility and robustness in complex production systems within
   Industry 4.0. Multi-Agent systems often use auction mechanisms to
   allocate tasks and resources. Each agent calculates their bids
   independently. This paper presents a concept to generate additional
   benefits for a company which hosts more than one agent in an Industry
   4.0 market-place. The optimization is based on a learning algorithm
   which estimates probabilities for future orders from an order history.
   The concept was tested in a simulation and found to offer considerable
   benefits.}},
