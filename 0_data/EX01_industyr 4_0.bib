@article{ ISI:000414881500014,
Author = {Dieber, Bernhard and Breiling, Benjamin and Taurer, Sebastian and
   Kacianka, Severin and Rass, Stefan and Schartner, Peter},
Title = {{Security for the Robot Operating System}},
