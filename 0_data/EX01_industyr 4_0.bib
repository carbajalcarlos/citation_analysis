@article{ ISI:000426881400014,
Author = {Barth, Mike},
Title = {{Micro-computers in modular plants - Lean automation of a model plant}},
Journal = {{ATP EDITION}},
Year = {{2017}},
Number = {{3}},
Pages = {{30-39}},
Abstract = {{Digitalization, digital transformation, Cyber Physical Systems, Industry
   4.0 and the Internet of Things lead to new technical requirements for
   the automation of machines and industrial plants. An example is the need
   for more flexible plant structures capable of producing small batches in
   conjunction with the fastest possible reconfiguration of the plant. A
   technical solution is the introduction of modularised industrial plants
   with defined external interfaces for the combination of modules. This
   paper describes investigations of the implementation variants of
   modularization for lean automation architectures. Here, the direct
   module communication as well as the use of the latest microcomputer
   technology is demonstrated. The resulting openness of this new
   technology allows for the use of high level services, which is shown for
   the example of digital plant twins.}},
