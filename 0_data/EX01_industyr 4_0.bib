@article{ ISI:000387438200002,
Author = {Harrison, Robert and Vera, Daniel and Ahmad, Bilal},
Title = {{Engineering the Smart Factory}},
Journal = {{CHINESE JOURNAL OF MECHANICAL ENGINEERING}},
Year = {{2016}},
Volume = {{29}},
Number = {{6}},
Pages = {{1046-1051}},
Month = {{NOV}},
Abstract = {{The fourth industrial revolution promises to create what has been called
   the smart factory. The vision is that within such modular structured
   smart factories, cyber-physical systems monitor physical processes,
   create a virtual copy of the physical world and make decentralised
   decisions. This paper provides a view of this initiative from an
   automation systems perspective. In this context it considers how future
   automation systems might be effectively configured and supported through
   their lifecycles and how integration, application modelling,
   visualisation and reuse of such systems might be best achieved. The
   paper briefly describes limitations in current engineering methods, and
   new emerging approaches including the cyber physical systems (CPS)
   engineering tools being developed by the automation systems group (ASG)
   at Warwick Manufacturing Group, University of Warwick, UK.}},
