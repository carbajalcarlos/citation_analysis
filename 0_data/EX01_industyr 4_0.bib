@article{ ISI:000383928800004,
Author = {Monostori, L. and Kadar, B. and Bauernhansl, T. and Kondoh, S. and
   Kumara, S. and Reinhart, G. and Sauer, O. and Schuh, G. and Sihn, W. and
   Ueda, K.},
Title = {{Cyber-physical systems in manufacturing}},
