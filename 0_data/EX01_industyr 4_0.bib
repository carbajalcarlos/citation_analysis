@article{ ISI:000417206600002,
Author = {Wassermann, Erik and Fay, Alexander},
Title = {{Test and verification methods for multi-agent systems - status quo and
   further challenges}},
Journal = {{AT-AUTOMATISIERUNGSTECHNIK}},
Year = {{2017}},
Volume = {{65}},
Number = {{11, SI}},
Pages = {{721-736}},
Month = {{NOV}},
Abstract = {{Multi-Agent Systems are a promising technology to cope with the
   requirements of digitalization, e.g., in CPS, IoT and Industry 4.0.
   Although intensive research has been carried out on software agents
   during the last two decades, they have not found broad acceptance in
   industry yet. One reason are the demands of industrial technical
   applications regarding safety, real-time response and reliability.
   Within this contribution, an overview of those test and verification
   methods is given which have been especially developed for Multi Agent
   Systems, and their merits and limitations are pointed out. Thus,
   assistance should be provided regarding the suitability of these test
   and verification methods for particular needs of developers. This should
   foster the wider acceptance of Multi Agent Systems in industrial
   practice and point out directions for further research.}},
