@article{ ISI:000426881400015,
Author = {Berg, Julia and Richter, Christoph and Spiegelberger, Bernd and
   Reinhart, Gunther},
Title = {{Developing user-centred mechatronic systems - A reference model}},
Journal = {{ATP EDITION}},
Year = {{2017}},
Number = {{3}},
Pages = {{40-49}},
Abstract = {{Due to recent trends in the context of digitalization and Industry 4.0,
   mechatronic systems are becoming increasingly complicated in terms of
   their use. In order to meet this challenge, systems are needed that can
   be handled by operators without additional efforts. This can be achieved
   by consideration of the role of operators throughout the development
   process. A reference model for the user-centred development of
   mechatronics systems was elaborated within the PEBeMA research project.
   The main elements of this reference model are presented in this paper,
   along with the fundamental development procedure and concrete
   development actions.}},
Publisher = {{DIV DEUTSCHER INDUSTRIEVERLAG GMBH}},
