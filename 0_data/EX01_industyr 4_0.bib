@incollection{ ISI:000419768000006,
Author = {Manogaran, Gunasekaran and Thota, Chandu and Lopez, Daphne and
   Sundarasekar, Revathi},
Editor = {{Thames, L and Schaefer, D}},
Title = {{Big Data Security Intelligence for Healthcare Industry 4.0}},
