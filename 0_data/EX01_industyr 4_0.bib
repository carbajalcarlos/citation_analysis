@article{ ISI:000433478300019,
Author = {Haus, Christina and Drumm, Oliver},
Title = {{Field devices and digital plant - More than catalogue data}},
Journal = {{ATP EDITION}},
Year = {{2015}},
Number = {{12}},
Pages = {{30-39}},
Abstract = {{Digital representations of production plants are an essential building
   block in the context of Industry 4.0. The so-called digital plant has to
   be augmented and maintained over all engineering and operation phases
   throughout the operational life of the plant. Field devices have a
   strong influence on the functionality and dynamics of a production
   plant. Even today, the challenge of consolidating the necessary
   information for use in the digital plant is still underestimated. This
