@article{ ISI:000426878100019,
Author = {Otten, Wilhelm},
Title = {{Industry 4.0 and digitalisation What is the potential for the chemical
   industry?}},
Journal = {{ATP EDITION}},
Year = {{2016}},
Number = {{1-2}},
Pages = {{28-32}},
Abstract = {{The discussion on Industry 4.0, the Internet of Things, and
   digitalisation in general is marked by the uncertainty in the
   engineering sector about the future impact on production technology and
   business. The initiative for Industry 4.0 came primarily from production
   engineering, with the processing sector apparently at first only
   onlookers. Therefore there have been few cogent responses about how
   digitalisation will impact on our production and processes, and where
   the advantages lie for the chemical industry. The major chemical
   companies have meanwhile responded and set up working groups on
   digitalisation and Industry 4.0. In this article, an attempt is made to
   analyse developments and to draw conclusions about the possible impact
   of Industry 4.0 and digitalisation on the processing industry, and in
   particular the economic consequences.}},
