Author = {Trappey, Amy J. C. and Trappey, Charles V. and Govindarajan, Usharani
   Hareesh and Sun, John J. and Chuang, Allen C.},
Title = {{A Review of Technology Standards and Patent Portfolios for Enabling
   Cyber-Physical Systems in Advanced Manufacturing}},
