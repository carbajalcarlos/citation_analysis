@article{ ISI:000426879500036,
Author = {Hoffmann, Max and Buescher, Christian and Meisen, Tobias and Jeschke,
   Sabina},
Title = {{OPC UA based multi-agent systems - Intelligent automation solutions for
   traditional production infrastructures}},
Journal = {{ATP EDITION}},
Year = {{2016}},
Number = {{7-8}},
Pages = {{46-57}},
Abstract = {{This article examines an approach for the intelligent coupling of a
   digital production site based on OPC UA and the agent paradigm. By
   integrating the interface standard OPC UA with methods of distributed
   artificial intelligence, interoperability and vertical integration of
   traditional production infrastructures using secure and reliable
   approaches becomes possible while also enabling scenarios of Industry
   4.0. We describe an integration approach of the physical and digital
   world based on a representation of intelligent software agents using the
   object-oriented modelling concepts of OPC UA.}},
