@article{ ISI:000306826800002,
Author = {Karpischek, Stephan and Michahelles, Florian and Fleisch, Elgar},
Title = {{my2cents: enabling research on consumer-product interaction}},
Journal = {{PERSONAL AND UBIQUITOUS COMPUTING}},
Year = {{2012}},
Volume = {{16}},
Number = {{6, SI}},
Pages = {{613-622}},
Month = {{AUG}},
Note = {{International Workshop on Smartphone Applications and Services
   (Smartphone), Gwangju, SOUTH KOREA, DEC 09-11, 2010}},
Abstract = {{Barcode scanners for smartphones enable mobile product-centric services
   for consumers. We have developed a mobile app that enables consumers to
   share their use of and opinions about products with their friends and
   others. Our goal is to establish a product-centric information stream
