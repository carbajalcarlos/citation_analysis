@article{ ISI:000405533300012,
Author = {Schillo, R. Sandra and Kinder, Jeffrey S.},
Title = {{Delivering on societal impacts through open innovation: a framework for
   government laboratories}},
Journal = {{JOURNAL OF TECHNOLOGY TRANSFER}},
