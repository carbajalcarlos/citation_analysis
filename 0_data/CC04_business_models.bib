@article{ ISI:000327813700005,
Author = {Li, Z. and Huang, George Q. and Fang, J. and Qu, T.},
Title = {{Ontology-based dynamic alliance services (ODAS) in production service
   system}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTER INTEGRATED MANUFACTURING}},
