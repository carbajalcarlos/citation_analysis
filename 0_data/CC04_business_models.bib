@article{ ISI:000337170100007,
Author = {Telang, Pankaj R. and Kalia, Anup K. and Singh, Munindar P.},
Title = {{Engineering Service Engagements via Commitments}},
Journal = {{IEEE INTERNET COMPUTING}},
Year = {{2014}},
Volume = {{18}},
Number = {{3}},
Pages = {{46-54}},
Month = {{MAY-JUN}},
Abstract = {{A service engagement describes how two or more independent parties
   interact with each other. Traditional approaches specify these
   interactions as message sequence charts (MSCs), hiding underlying
   business relationships and, consequently, complicating modification.
   Comma is a commitment-based approach that produces a business model
   drawn from an extensible pattern library and yields flexible MSCs. An
   empirical study shows that models produced via Comma yield superior
   flexibility, are comprehensible to others, and take less time and effort
   to produce.}},
