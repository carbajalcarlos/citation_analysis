@article{ ISI:000412920200027,
Author = {Zhang, Chao and Guan, Jiancheng},
Title = {{How to identify metaknowledge trends and features in a certain research
   field? Evidences from innovation and entrepreneurial ecosystem}},
Journal = {{SCIENTOMETRICS}},
