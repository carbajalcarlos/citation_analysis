@article{ ISI:000416146800005,
Author = {Axelson, Mattias and Netz, Joakim and Sandstrom, Christian},
Title = {{Collective action problems in public sector innovation: A business model
   perspective}},
Journal = {{CREATIVITY AND INNOVATION MANAGEMENT}},
Year = {{2017}},
Volume = {{26}},
Number = {{4}},
Pages = {{370-378}},
Month = {{DEC}},
Abstract = {{We explore how the literature on business models can explain the
   outcomes of innovation attempts in the public sector. Our findings
   suggest that governments can access a well-developed knowledge domain
   for a public sector area but have a weak ability to propagate its value
   for society. Drawing on the business model literature concerning
   interdependence and distributed agency, we illustrate how a collective
   action problem related to innovation may arise in the public sector. We
   illustrate this new category of public innovation challenge with the
   (failed) case of the Swedish civil contingencies system and subsequently
   discuss a new line of inquiry for future research.}},
