@article{ ISI:000433380700025,
Author = {Li, Juanjuan and Ni, Xiaochun and Yuan, Yong},
Title = {{The Reserve Price of Ad Impressions in Multi-Channel Real-Time Bidding
   Markets}},
Journal = {{IEEE TRANSACTIONS ON COMPUTATIONAL SOCIAL SYSTEMS}},
