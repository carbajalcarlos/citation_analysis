@incollection{ ISI:000427544600005,
Author = {Greerer, Will and Smith, Gregory and Hyland, David and Frolick, Mark},
Editor = {{Lawrence, KD and Klimberg, RK}},
Title = {{DIGITAL DISRUPTION How E-Commerce Is Changing the Grocery Game}},
Booktitle = {{CONTEMPORARY PERSPECTIVES IN DATA MINING, VOL 3}},
Series = {{Contemporary Perspectives in Data Mining}},
Year = {{2018}},
Volume = {{3}},
Pages = {{67-78}},
Abstract = {{The traditional grocery store business model is in a state of
   disruption, driven by the transition to selling groceries online via
   e-commerce. While online grocery sales are still minimal, they arc
   expected to grow substantially as more retailers begin to compete in
   this space on convenience, access, and price. This article explores the
   primary methods of e-commerce product deployment in the grocery industry
   as well as the benefits and challenges posed by this new data-driven
   business model. To maximize its growth potential, the grocery e-commerce
   model may ultimately need to transition away from the existing store
   model where consumers gain access to products in traditional
   brick-and-mortar, high-dispersed environments to a new distribution
   system wholly reliant on information technology.}},
