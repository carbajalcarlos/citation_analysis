@article{ ISI:000414879300062,
Author = {Tong, Xin and Tao, Dongyan and Lifset, Reid},
Title = {{Varieties of business models for post-consumer recycling in China}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
