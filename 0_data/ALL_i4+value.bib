@article{ ISI:000433475600015,
Author = {Wollschlaeger, Martin and Lehmann, Robert and Dennert, Alexander},
Title = {{Representing life cycle related information in Industry 4.0 - Models,
   representation, and management}},
Journal = {{ATP EDITION}},
Year = {{2015}},
Number = {{5}},
Pages = {{24-31}},
Abstract = {{A flexible system structure is an important requirement for Industry
   4.0. Different components are to be correlated according to the value
   chains. It is very important to make life cycle related information
   about the components available in a homogeneous way (e.g. version
   information, usable services, application history). The article presents
   a base model for life cycle information and discusses various
   representation formats. This enables software tools to gain access to
   this information. A specific focus is placed on the operation phase.}},
