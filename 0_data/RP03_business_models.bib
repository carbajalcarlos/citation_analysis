@article{ ISI:000376237400002,
Author = {Purkayastha, Anish and Sharma, Sunil},
Title = {{Gaining competitive advantage through the right business model: analysis
   based on case studies}},
Journal = {{JOURNAL OF STRATEGY AND MANAGEMENT}},
