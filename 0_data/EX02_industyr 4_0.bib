@article{ ISI:000429106500005,
Author = {Urbina, Marcelo and Astarloa, Armando and Lazaro, Jesus and Acosta,
   Tatiana},
Title = {{CPPS Gateway: Implementation of Modbus and Profibus on a Programmable
   SoC Platform}},
Journal = {{IEEE LATIN AMERICA TRANSACTIONS}},
Year = {{2018}},
Volume = {{16}},
Number = {{2}},
Pages = {{335-341}},
Month = {{FEB}},
Abstract = {{The communication needs of the different stages of production proposed
   in the new industrial generation (INDUSTRY 4.0) demand devices that can
   handle high availability protocols with industrial characteristics. This
   work presents an architecture of a CPPS gateway that can be implemented
   in a SoC platform and allows to establish industrial communication
   Modbus and Profibus with the different elements that can be found in an
   industrial environment. Considering the software and hardware
   flexibility that the architecture should have, FPGAs and reconfigurable
   devices, in general, are the best candidates for the implementation. An
   experimental part that is implemented by means of Electronic Design
   Automation (EDA) tools and Python libraries developed by third parties
   is presented, to achieve a device that handles Profibus and Modbus
   communications.}},
