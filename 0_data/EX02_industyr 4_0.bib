@article{ ISI:000428512000019,
Author = {Yan, Zhu-wen and Wang, Bao-sheng and Bu, He-nan and Zhang, Dian-hua},
Title = {{Intelligent assignation strategy of collaborative optimization for
   flatness control}},
Journal = {{JOURNAL OF THE BRAZILIAN SOCIETY OF MECHANICAL SCIENCES AND ENGINEERING}},
Year = {{2018}},
Volume = {{40}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{Under the background of industry 4.0, advanced strip control process is
   a central part of rolling intelligent manufacturing. In the mainstream
   rolling control process, the work roll bending and the intermediate roll
   bending are turned on at the same time. Both stepwise methods and
   alternative methods are conducted by sacrificing adjustment ability. In
   this paper, the dimension of influencing factors is increased by
   considering adjustment direction as constraint operator. In the rolling
   control process, a new intelligent assignation strategy of collaborative
   optimization based on artificial neural networks and Topkis-Veinott has
   been proposed. In AINTV collaborative optimization, the thought patterns
   of searching and the thought patterns of learning are combined. Five
   field test experiments are conducted and the flatness in different
   rolling stages and in different strip area is analyzed.}},
