@article{ ISI:000428237100053,
Author = {Toquica, Juan S. and Zivanovic, Sasa and Alvares, Alberto J. and
   Bonnard, Renan},
Title = {{A STEP-NC compliant robotic machining platform for advanced
   manufacturing}},
