@article{ ISI:000207796100004,
Author = {Samavi, Reza and Yu, Eric and Topaloglou, Thodoros},
Title = {{Strategic reasoning about business models: a conceptual modeling
   approach}},
Journal = {{INFORMATION SYSTEMS AND E-BUSINESS MANAGEMENT}},
Year = {{2009}},
Volume = {{7}},
Number = {{2, SI}},
Pages = {{171-198}},
Month = {{MAR}},
Abstract = {{Strategic reasoning about business models is an integral part of service
   design. In fast moving markets, businesses must be able to recognize and
   respond strategically to disruptive change. They have to answer
   questions such as: what are the threats and opportunities in emerging
   technologies and innovations? How should they target customer groups?
   Who are their real competitors? How will competitive battles take shape?
   In this paper we define a strategic modeling framework to help
   understand and analyze the goals, intentions, roles, and the rationale
   behind the strategic actions in a business environment. This
   understanding is necessary in order to improve existing or design new
   services. The key component of the framework is a strategic business
   model ontology for representing and analyzing business models and
   strategies, using the i{*} agent and goal oriented methodology as a
   basis. The ontology introduces a strategy layer which reasons about
   alternative strategies that are realized in the operational layer. The
   framework is evaluated using a retroactive example of disruptive
   technology in the telecommunication services sector from the literature.}},
