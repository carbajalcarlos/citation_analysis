@article{ ISI:000335564100002,
Author = {Cherif, Emna and Grant, Delvin},
Title = {{Analysis of e-business models in real estate}},
Journal = {{ELECTRONIC COMMERCE RESEARCH}},
Year = {{2014}},
Volume = {{14}},
Number = {{1}},
Pages = {{25-50}},
Month = {{MAR}},
Abstract = {{We use Canvas Application Framework to analyze e-business models of
   Realestate.yahoo.com, Yahoo.com, Redfin.com, Realtor.com, Trulia.com,
   Zillow.com, and Craigslist.com. We investigate the similarities and
   differences among e-business models and how e-business model
   differentiation affects company success. We looked at which business
   model components, such as distribution channels, business partnerships,
   attracting customers, revenue streams, etc., are important. We further
   investigate whether the web-advertising, brokerage, virtual value-chain,
   or the diversified internet business model is used by each company, and
   which ones are more successful. We discover similarities and differences
   among e-business models. Differences include the use of revenue streams
   for generating revenue, the extent of partnerships, and the role they
   play in a company's success.}},
