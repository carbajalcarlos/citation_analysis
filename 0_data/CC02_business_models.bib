@article{ ISI:000353934700011,
Author = {Davis, Jeffrey R. and Richard, Elizabeth E. and Keeton, Kathryn E.},
Title = {{Open Innovation at NASA A New Business Model for Advancing Human Health
   and Performance Innovations}},
Journal = {{RESEARCH-TECHNOLOGY MANAGEMENT}},
Year = {{2015}},
Volume = {{58}},
Number = {{3}},
Pages = {{52-58}},
Month = {{MAY-JUN}},
Abstract = {{This paper describes a new business model for advancing NASA human
   health and performance innovations and demonstrates how open innovation,
   including the use of crowdsourcing and technology solution sourcing
   services, shaped its development. A 45 percent research and technology
   development budget reduction drove formulation of a strategic plan
   grounded in collaboration. We describe the strategy execution, including
   adoption and results of open innovation initiatives, the challenges of
   culture change, and the development of a knowledge management tool to
   educate and engage the workforce in the new strategy and promote culture
   change.}},
