@article{ ISI:000239725500003,
Author = {Singh, Ajit and Zammit, Ann},
Title = {{Corporate governance, crony capitalism and economic crises: should the
   US business model replace the Asian way of ``doing business{''}?}},
Journal = {{CORPORATE GOVERNANCE-AN INTERNATIONAL REVIEW}},
Year = {{2006}},
Volume = {{14}},
Number = {{4}},
Pages = {{220-233}},
Month = {{JUL}},
Abstract = {{This paper considers the Greenspan/Summers/IMF (GSI) argument that the
   Asian way of doing business was the deep cause of the Asian crisis. The
   IMF reform programme for the crisis-affected Asian countries suggested
   they should abandon the Asian business model and adopt the US corporate
   model. The main findings are: (a) contrary to GSI doctrine, poor
   corporate governance and lack of competition are not common
   characteristics of the Asian business model; (b) the stock market-based
   US business model has severe limitations for developing country
   corporations, not least because of imperfect share prices and the
   imperfect market for corporate control.}},
