@article{ ISI:000306024100002,
Author = {Lenert, Leslie and Sundwall, David and Lenert, Michael Edward},
Title = {{Shifts in the architecture of the Nationwide Health Information Network}},
Journal = {{JOURNAL OF THE AMERICAN MEDICAL INFORMATICS ASSOCIATION}},
Year = {{2012}},
Volume = {{19}},
Number = {{4}},
Pages = {{498-502}},
Month = {{JUL}},
Abstract = {{In the midst of a US \$30 billion USD investment in the Nationwide
   Health Information Network (NwHIN) and electronic health records
   systems, a significant change in the architecture of the NwHIN is taking
   place. Prior to 2010, the focus of information exchange in the NwHIN was
   the Regional Health Information Organization (RHIO). Since 2010, the
   Office of the National Coordinator (ONC) has been sponsoring policies
   that promote an internet-like architecture that encourages point
   to-point information exchange and private health information exchange
   networks. The net effect of these activities is to undercut the limited
   business model for RHIOs, decreasing the likelihood of their success,
   while making the NwHIN dependent on nascent technologies for community
   level functions such as record locator services. These changes may
   impact the health of patients and communities. Independent,
   scientifically focused debate is needed on the wisdom of ONC's proposed
   changes in its strategy for the NwHIN.}},
