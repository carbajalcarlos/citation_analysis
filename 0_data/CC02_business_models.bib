@article{ ISI:000321970400004,
Author = {Huang, Keman and Fan, Yushun and Tan, Wei and Qian, Minghui},
Title = {{BSNet: a network-based framework for service-oriented business ecosystem
   management}},
Journal = {{CONCURRENCY AND COMPUTATION-PRACTICE \& EXPERIENCE}},
Year = {{2013}},
Volume = {{25}},
Number = {{13, SI}},
Pages = {{1861-1878}},
Month = {{SEP 10}},
Note = {{2nd international workshop on workflow management in service and cloud
   computing, Hong Kong, PEOPLES R CHINA, DEC 11-13, 2010}},
Abstract = {{As enterprises turning to SOA, services-oriented business ecosystem
   (SOBE) has become an important pattern for the organization and
   management of the massive business services. At the same time, the
   emergence of Internet of Services (IOS) provides a business model in
   which service vendors and consumers can interact with each other via the
   Internet. This paradigm makes it possible that the services in SOBE are
   managed in an autonomous and coordinated manner. The challenge here is
   to organize these massive business services, coordinate, and federate
   them to achieve the benefits of SOA. To address these challenges, this
   paper presents BSNet, a framework on the basis of the service
   correlation networks to manage the SOBE. The model consists of a
   who-what-how service correlation network that captures the various
   relations in SOBE. Finally, a prototype system is developed, and a
   simulated case study is provided to show the expanded value of our
   network-based framework. Copyright (c) 2013 John Wiley \& Sons, Ltd.}},
