@article{ ISI:000275196700008,
Author = {Sabatier, V. and Mangematin, V. and Rousselle, T.},
Title = {{Orchestrating networks in the biopharmaceutical industry: small hub
   firms can do it}},
Journal = {{PRODUCTION PLANNING \& CONTROL}},
Year = {{2010}},
Volume = {{21}},
Number = {{2}},
Pages = {{218-228}},
Abstract = {{The biopharmaceutical industry has been one of the most dynamic and
   promising sectors. The entry of Biotechnology start-ups in the 1980s led
   to the reconfiguration of the drug development value chain and the
   emergence of new competences. As the sector evolved, specialised firms
   appeared. As the industry matures, the production process becomes more
   specialised to support optimisation of technological steps. Our case
   studies reveal that the coordination of networks can be specialised,
   with the emergence of Dedicated Coordinating Firms. On the basis of four
   case studies of European biotechnology companies within a business model
   approach, this article helps understanding how coordinating a network
   can be successful and how small hub firms can do it.}},
Publisher = {{TAYLOR \& FRANCIS LTD}},
Address = {{4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Sabatier, V (Reprint Author), Univ Grenoble 2, GAEL, UMR 1215, F-38000 Grenoble, France.
